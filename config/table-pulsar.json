{
    "tableName": "wikipedia",
    "tableType": "REALTIME",
    "segmentsConfig": {
      "timeColumnName": "ts",
      "schemaName": "wikipedia",
      "replicasPerPartition": "1"
    },
    "tenants": {},
    "tableIndexConfig": {
      "streamConfigs": {
        "streamType": "pulsar",
        "stream.pulsar.topic.name": "wiki-events",
        "stream.pulsar.bootstrap.servers": "pulsar://pulsar:6650",
        "stream.pulsar.consumer.type": "lowlevel",
        "stream.pulsar.fetch.timeout.millis": "10000",
        "stream.pulsar.consumer.prop.auto.offset.reset": "smallest",
        "stream.pulsar.consumer.factory.class.name": "org.apache.pinot.plugin.stream.pulsar.PulsarConsumerFactory",
        "stream.pulsar.decoder.class.name": "org.apache.pinot.plugin.stream.kafka.KafkaJSONMessageDecoder",
        "realtime.segment.flush.threshold.rows": "1000",
        "realtime.segment.flush.threshold.time": "24h",
        "realtime.segment.flush.segment.size": "100M"
      }
    },
    "metadata": {},
    "ingestionConfig": {
      "transformConfigs": [
        {
          "columnName": "id",
          "transformFunction": "JSONPATH(meta, '$.id')"
        },
        {
          "columnName": "stream",
          "transformFunction": "JSONPATH(meta, '$.stream')"
        },
        {
          "columnName": "domain",
          "transformFunction": "JSONPATH(meta, '$.domain')"
        },
        {
          "columnName": "topic",
          "transformFunction": "JSONPATH(meta, '$.topic')"
        },
        {
          "columnName": "uri",
          "transformFunction": "JSONPATH(meta, '$.uri')"
        },
        {
          "columnName": "ts",
          "transformFunction": "\"timestamp\" * 1000"
        }
      ]
    }
  }